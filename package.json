{
  "publisher": "xhofe",
  "name": "unescape",
  "displayName": "Unescape",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "",
  "author": "Andy Hsu <i@nn.ci>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/xhofe",
  "homepage": "https://github.com/xhofe/vscode-unescape#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xhofe/vscode-unescape"
  },
  "bugs": {
    "url": "https://github.com/xhofe/vscode-unescape/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/xhofe"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "l10n/*",
    "res/*",
    "package.nls.*.json",
    "package.nls.json"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "unescape.preview",
        "title": "%unescape.preview.title%"
      },
      {
        "command": "unescape.previewJson",
        "title": "%unescape.previewJson.title%"
      }
    ],
    "keybindings": [
      {
        "command": "unescape.preview",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "editorTextFocus"
      },
      {
        "command": "unescape.previewJson",
        "key": "ctrl+alt+j",
        "mac": "cmd+alt+j",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "unescape.preview",
          "group": "1_modification"
        },
        {
          "command": "unescape.previewJson",
          "group": "1_modification"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "unescape",
      "properties": {
        "unescape.jsonIndentSize": {
          "type": "number",
          "default": 2,
          "minimum": 0,
          "maximum": 8,
          "description": "Number of spaces to use for JSON indentation (0 for compact)"
        },
        "unescape.statusBarEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Show status bar information when cursor is inside a string"
        }
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp",
    "ext:package": "vsce package --no-dependencies",
    "ext:publish": "vsxpub --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.19.3",
    "@types/vscode": "^1.107.0",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "ovsx": "^0.10.7",
    "pnpm": "^10.27.0",
    "reactive-vscode": "^0.2.22",
    "taze": "^19.9.2",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.5.1",
    "vsxpub": "^0.1.2"
  }
}